---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>GEDCOM X</title>
 <link href="http://familysearch.github.com/gedcomx/atom.xml" rel="self"/>
 <link href="http://familysearch.github.com/gedcomx/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://familysearch.github.com/gedcomx/</id>
 <author>
   <name>Ryan Heaton</name>
   <email>heatonra@familysearch.org</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://familysearch.github.com/gedcomx{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://familysearch.github.com/gedcomx/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
   {% if post.author %}
   <author>
     <name>{{ post.author }}</name>
     {% if post.author_email %}
     <email>{{ post.author_email }}</email>
     {% endif %}
   </author>
   {% endif %}
 </entry>
 {% endfor %}
 
</feed>
